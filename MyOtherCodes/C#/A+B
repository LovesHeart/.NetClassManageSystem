using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
//可以不用namespace a{} 括起来
class APlusB
{
    static void Main(string[] args)
    {
        string[] input = Console.ReadLine().Split(' ');
        Console.WriteLine(int.Parse(input[0]) + int.Parse(input[1]));
    }
}
